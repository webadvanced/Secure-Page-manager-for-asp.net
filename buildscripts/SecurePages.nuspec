    <language>en-US</language>
    <dependencies>
      <dependency id="WebActivator" version="1.5" />
    </dependencies>
  </metadata>
  <files>
    <file src="lib\content\**\*.pp" target="content" />
    <file src="lib\content\**\*.txt" target="content" />
